import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      int A=10,B=20,C=30,ans=0;
     int arr[]=new int[3];
    arr[0]=A;
    arr[1]=B;
    arr[2]=C;
   Arrays.sort(arr);
   arr[0]*=10000;
   arr[1]*=100;
   ans=arr[0]+arr[1]+arr[2];
   System.out.print(ans);
    }
}