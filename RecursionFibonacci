import java.util.*;
public class Main{
    public static int recursionFibonacci(int a){
        if((a==1) || (a==2)){
            return 1;
        }
        return recursionFibonacci(a-2)+recursionFibonacci(a-1);
    }
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int ans=recursionFibonacci(a);
        System.out.println(ans);
    }
}