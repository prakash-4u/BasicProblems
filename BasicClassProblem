import java.util.*;
public class Student {
    int id;
    String name;
    int m1,m2,m3;
    public static void main(String args[]) {
      Student st1=new Student();
      st1.id=1;
      st1.name="Prakash";
      st1.m1=10;
      st1.m2=20;
      st1.m3=30;
      System.out.println(st1.id);
      System.out.println(st1.name);
      System.out.println(st1.m1);
      System.out.println(st1.m2);
      System.out.println(st1.m3);
    }
}