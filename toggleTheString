import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      Scanner sc=new Scanner(System.in);
      String s=sc.nextLine();
      int a=0;
     String ch="";
      int n=s.length();
      for(int i=0;i<n;i++){
          if(s.charAt(i)>=65 && s.charAt(i)<=90){
             ch+=(char) (s.charAt(i)+32);
             
          }
          else{
              ch+=(char) (s.charAt(i)-32);
          }
      }
       System.out.println(ch);
}
}