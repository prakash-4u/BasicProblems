import java.util.*;
public class Student {
    int id;
    String name;
    int m1,m2,m3;
    int totalmarks(){
        return m1+m2+m3;
    }
    int maxMarks(){
        return Math.max(m1,Math.max(m2,m3));
    }
    void printName(){
        System.out.println(name);
    }
    public static void main(String args[]) {
      Student st3=null;
      st3.m1=10;
      System.out.println(st3.m1);
    }
}